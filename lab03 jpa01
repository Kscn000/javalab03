import java.util.*;
public class jpa01 {

    public static void main(String[] args) {
        System.out.println("Input:");
        int m= 0 ,n=new Scanner(System.in).nextInt();
        for (int i=1;i<=n;i++){
            m=m+i;

        }
        System.out.println("1 + ... + " + n +" = " + m);

    }
}
